{
    "name": "grpc-migration",
    "version": "4.2.0",
    "description": "grpc migration",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "echo 'No e2e tests implemented yet.'",
        "migrate:create": "cross-var migrate create --template-file ./src/migrations/utils/template.ts --migrations-dir=\"./src/migrations/files/$npm_package_version\" -F --compiler=\"ts:./src/migrations/utils/ts-compiler.js\"",
        "migrate:up_100": "cross-var migrate --migrations-dir=\"./src/migrations/files/1.0.0\" --store=\"./src/migrations/utils/storage.ts\" -F --compiler=\"ts:./src/migrations/utils/ts-compiler.js\" up",
        "migrate:down_100": "cross-var migrate --migrations-dir=\"./src/migrations/files/1.0.0\" --store=\"./src/migrations/utils/storage.ts\" -F --compiler=\"ts:./src/migrations/utils/ts-compiler.js\" down",
        "migrate:up": "cross-var migrate --migrations-dir=\"./src/migrations/files/$npm_package_version\" --store=\"./src/migrations/utils/storage.ts\" -F --compiler=\"ts:./src/migrations/utils/ts-compiler.js\" up",
        "migrate:down": "cross-var migrate --migrations-dir=\"./src/migrations/files/$npm_package_version\" --store=\"./src/migrations/utils/storage.ts\" -F --compiler=\"ts:./src/migrations/utils/ts-compiler.js\" down"
    },
    "dependencies": {
        "@babel/core": "^7.24.5",
        "@nestjs/common": "10.2.7",
        "@nestjs/core": "10.2.7",
        "@nestjs/cqrs": "^10.2.6",
        "@nestjs/jwt": "^10.1.1",
        "@nestjs/microservices": "10.2.7",
        "@nestjs/mongoose": "^10.0.1",
        "@nestjs/platform-express": "10.2.7",
        "JSONStream": "^1.3.5",
        "aws-sdk": "^2.1218.0",
        "config": "^3.3.7",
        "cross-var": "^1.1.0",
        "cryptr": "^6.3.0",
        "dayjs": "^1.11.5",
        "flatted": "^3.2.9",
        "json5": "2.2.1",
        "lodash": "^4.17.21",
        "migrate": "^1.8.0",
        "mongoose": "6.11.3",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "7.5.5",
        "webpack": "^5.74.0"
    },
    "devDependencies": {
        "@nestjs/cli": "10.1.18",
        "@nestjs/schematics": "10.0.2",
        "@nestjs/testing": "10.2.7",
        "@types/express": "4.17.13",
        "@types/node": "18.0.3",
        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "5.30.5",
        "@typescript-eslint/parser": "5.30.5",
        "eslint": "8.19.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "jest": "28.1.2",
        "prettier": "2.7.1",
        "supertest": "6.2.4",
        "ts-jest": "28.0.5",
        "ts-loader": "9.3.1",
        "ts-node": "10.8.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.2.2"
    }
}
